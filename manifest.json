{
  "name": "Web Crawler",
  "description": "Web Crawler",
  "manifest_version": 2,
  "version": "1.0.0",
  "background": {
    "scripts": [
      "jquery-3.6.0.js",
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "assets/**/*"
  ],
  "permissions": [
    "<all_urls>",
    "webRequest",
    "unlimitedStorage",
    "storage",
    "downloads",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ]
}
